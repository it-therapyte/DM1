{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "http://dm-matrix.com/procedures",
  "description": "Процедуры",
  "title": "procedures.csv (opt)",
  "type": "object",
  "properties": {
    "patient-id": {
      "$ref": "../_common-types.json#/$defs/patient-id",
      "title": "ID пациента"
    },
    "visit-id": {
      "$ref": "../_common-types.json#/$defs/visit-id",
      "title": "ID визита"
    },
    "name": {
      "type": "string",
      "title": "Наименование процедуры"
    },
    "date": {
      "type": "string",
      "title": "Дата процедуры",
      "format": "date"
    }
  },
  "required": ["patient-id", "visit-id", "name", "date"]
}
