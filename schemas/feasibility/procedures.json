{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "http://dm-matrix.com/procedures",
  "description": "Процедуры",
  "title": "procedures.csv (opt)",
  "type": "object",
  "properties": {
    "patientId": {
      "$ref": "../_common-types.json#/$defs/patientId",
      "title": "ID пациента"
    },
    "visitId": {
      "$ref": "../_common-types.json#/$defs/visitId",
      "title": "ID визита"
    },
    "name": {
      "type": "string",
      "title": "Наименование процедуры"
    },
    "date": {
      "type": "string",
      "title": "Дата процедуры",
      "format": "date"
    }
  },
  "required": ["patientId", "visitId", "name", "date"]
}
