{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://dm-matrix.com/visits",
  "description": "Patient demographic information",
  "type": "array",
  "items": {
    "$ref": "#/$defs/visit"
  },
  "$defs": {
    "visit": {
      "type": "object",
      "properties": {
        "patient-id": {
          "$ref": "../_common-types.json#/$defs/patient-id"
        },
        "visit-id": {
          "$ref": "../_common-types.json#/$defs/visit-id"
        },
        "visit-date": {
          "type": "string",
          "title": "Visit date",
          "format": "date"
        }
      }
    }
  }
}
