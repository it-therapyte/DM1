{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://dm-matrix.com/visit",
  "description": "Patient demographic information",
  "type": "object",
  "title": "visits.csv",
  "properties": {
    "patient-id": {
      "$ref": "../_common-types.json#/$defs/patient-id"
    },
    "visit-id": {
      "$ref": "../_common-types.json#/$defs/visit-id"
    },
    "visit-date": {
      "type": "string",
      "title": "Visit date",
      "format": "date"
    }
  },
  "required": ["patient-id", "visit-id", "visit-date"]
}
